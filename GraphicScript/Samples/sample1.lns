fun maker of Rect x:int y:int ->
    let r = new Rect ()
    r.X = (x + 1) * 50
    r.Y = (y + 1) * 50
    r.Focus = ->
        r.Fill = System.Windows.Media.Color::FromRgb
            <| (rand 100 255) as byte
            <| (rand 100 255) as byte
            <| (rand 100 255) as byte
    r

10.times
    <| (x:int) ->
        10.times
            <| (y:int) ->
                let c = maker x y
                Screen.Add c