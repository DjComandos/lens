fun maker of Figure id:int ->
    let curr = if(id % 2 == 0)
        new Rect () as Figure
    else
        new Circle ()

    curr.Position = new (10; id*50)
    curr.UpdateAction = ->
        curr.X = curr.X + id

    curr

let range = Enumerable::Range 1 10
let objs = range.Select ((x:int) -> maker(x))
Screen.Add objs